//Good case
int(2) res main(int(2) a, int(2) b, int(2) sel ) { 
    // when, select and other vhdl keywords need to have special string appended to them   
    switch ( sel ) { 
        case 0: res = a + b; 
        case 1: res = a + !b + 1; 
        case 2: res = a && b; 
        default: res = a || b;  
    }

} 
