const int(3) RED = 5; 


int(4) c adder ( int(4) a , int(4) b ) { 
       c = a + b;
}


// not assigning all the input args 
int(4) d main ( int(4) a, int(4) b, int(4) c ) { 

       // passing in consts 
       (d) = adder(6,RED); // note that this const is int(3), while the input arg signature says int(4)    


}

