// priority encoder 
int(3) code main ( int(8) sel ) { 

	if ( sel(0:0) ) { 
	   code = 0; 
	} 
	else if ( sel(1:1) ) { 
	     code = 1; 
	}
	else if ( sel(2:2) ) { 
	     code = 2; 
	} 
	else if ( sel(3:3) ) { 
	     code = 3; 
	} 
	else if ( sel(4:4) ) { 
	     code = 4; 
	} 
	else if ( sel(5:5) ) { 
	     code = 5; 
	} 
	else if ( sel(6:6) ) { 
	     code = 6; 
	} 
	else { 
	     code = 7;  	
	}	

}  
