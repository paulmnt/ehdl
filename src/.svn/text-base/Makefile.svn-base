OBJS = parser.cmo scanner.cmo sast.cmo clocktab.cmo ehdl.cmo 

ehdl : $(OBJS)
	ocamlc -o ehdl $(OBJS)

scanner.ml : scanner.mll
	ocamllex scanner.mll

parser.ml parser.mli : parser.mly
